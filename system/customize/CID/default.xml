  <category name="application">
    <module name="OOBE">
      <function name="Block_No_SIM">
        <set name="single">
          <item name="Default" type="boolean">no</item>
        </set>
      </function>
      <function name="Step_Config">
        <set name="single">
          <item name="Set_Language" type="boolean">yes</item>
          <item name="SIP_Tutorial" type="boolean">no</item>
          <item name="Internet_Connection" type="boolean">no</item>
          <item name="Wi-Fi" type="boolean">no</item>
          <item name="Location_Consent" type="boolean">no</item>
          <item name="VZW_Location_Consent" type="boolean">yes</item>
        </set>
      </function>
      <function name="Mobile_Data_Config">
        <set name="single">
          <item name="Default" type="boolean">no</item>
        </set>
      </function>
    </module>
    <module name="Crash_Report">
      <function name="setting">
        <set name="single">
          <item name="turn_off_report" type="boolean">yes</item>
          <item name="turn_off_tell_htc" type="boolean">yes</item>
          <item name="hide_usage_report" type="boolean">yes</item>
        </set>
      </function>
    </module>
    <module name="FOTA">
      <function name="settings">
        <set name="single">
          <item name="Enable_FOTA_UI" type="boolean">no</item>
          <item name="Enable_Manual_Check" type="boolean">no</item>
        </set>
      </function>
    </module>
    <module name="Wifihotspot">
      <function name="usermanagement">
        <set name="plenty" max="15">
          <item name="connection_array">10</item>
        </set>
      </function>
    </module>
    <module name="Lockscreen">
      <function name="shortcut">
        <set name="plenty">
          <item name="package">com.android.htccontacts</item>
          <item name="class">com.android.htccontacts.DialerTabActivity</item>
          <item name="order">1</item>
        </set>
        <set name="plenty">
          <item name="package">com.android.camera</item>
          <item name="class">com.android.camera.CameraEntry</item>
          <item name="order">2</item>
        </set>
        <set name="plenty">
          <item name="package">com.android.mms</item>
          <item name="class">com.android.mms.ui.MessageTabActivity</item>
          <item name="order">3</item>
        </set>
        <set name="plenty">
          <item name="package">com.htc.android.mail</item>
          <item name="class">com.htc.android.mail.MailListTab</item>
          <item name="order">4</item>
        </set>
      </function>
      <function name="shortcut_visibility">
        <set name="single" max="1">
          <item name="visibility">1</item>
        </set>
      </function>
    </module>
  </category>
  <BootConfiguration>
    <BootAnimation animation="/system/customize/resource/Vigor_VZW_bootanimation.zip" />
  </BootConfiguration>
  <ShutdownConfiguration>
    <ShutdownAnimation image_png="/system/customize/resource/hTC_downanimation.zip" image="" fps="20" />
  </ShutdownConfiguration>
  <category name="SettingsProvider">
    <module name="res">
      <function name="values">
        <set name="single">
          <item name="def_screen_off_timeout">45000</item>
          <item name="def_location_gps_providers_allowed">false</item>
          <item name="def_location_network_providers_allowed">false</item>
          <item name="def_inactivity_time">900</item>
        </set>
      </function>
    </module>
  </category>
  <category name="Settings">
    <module name="properties">
      <function>
        <set name="single">
          <item name="phone_finder_visibility">false</item>
        </set>
